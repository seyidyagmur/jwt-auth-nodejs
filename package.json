    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-boom": "^3.0.0",
    "glob": "^7.1.3",
    "jsonwebtoken": "^9.0.0",
    "md5": "^2.2.1",
    "mongoose": "^5.4.20",
