{
  "name": "auth-project",
  "version": "1.0.0",
  "description": "Auth with NodeJS and Angular 7",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client-install": "npm install --prefix auth-client",
    "server":"nodemon server.js --ignore tests",
    "client":"npm run start --prefix auth-client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
